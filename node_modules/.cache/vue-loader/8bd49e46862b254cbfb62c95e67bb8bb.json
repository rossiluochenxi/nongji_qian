-row>\r\n\r\n      <div class=\"box small-box blue\">\r\n        <!-- <p style=\"text-align: center;\">这是一个小盒子</p> -->\r\n       <p style=\"text-align: center;\"></p>\r\n       <table class=\"monitoring-table\" style=\"width:100%;\">\r\n        <caption>预警信息</caption>\r\n        <tr>\r\n            <th>养殖户名称</th>\r\n            <th>编号</th>\r\n            <th>品种</th>\r\n            <th>内容</th>\r\n        </tr>\r\n        <tr>\r\n            <td>养殖户1</td>\r\n            <td>001</td>\r\n            <td>品种1</td>\r\n            <td>预警内容1</td>\r\n        </tr>\r\n        <tr>\r\n            <td>养殖户2</td>\r\n            <td>002</td>\r\n            <td>品种2</td>\r\n            <td>预警内容2</td>\r\n        </tr>\r\n        <!-- 在这里添加更多的预警信息 -->\r\n    </table>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"right-column\">\r\n\r\n      <div class=\"box b-box blue\">\r\n        <p style=\"text-align: center;\">耳标在线数</p>\r\n       <div class=\"status-circle-container\">\r\n         <svg width=\"180\" height=\"180\">\r\n         <!-- 背景圆环 -->\r\n         <circle class=\"status-circle\" cx=\"90\" cy=\"90\" r=\"80\" stroke=\"#444\"></circle>\r\n         <!-- 在线圆环 -->\r\n         <circle id=\"online\" class=\"status-circle\" cx=\"90\" cy=\"90\" r=\"80\" stroke=\"#03a9f4\"></circle>\r\n         <!-- 离线圆环 -->\r\n         <circle id=\"offline\" class=\"status-circle\" cx=\"90\" cy=\"90\" r=\"80\" stroke=\"#f44336\"></circle>\r\n         <!-- 中心显示文字 -->\r\n        <text class=\"status-text\" x=\"90\" y=\"90\" text-anchor=\"middle\" alignment-baseline=\"middle\">在线/离线</text>\r\n         </svg>\r\n      </div>\r\n\r\n      </div>\r\n\r\n      <div class=\"box i-box blue\">\r\n        <p style=\"text-align: center;\">这是一个大盒子</p>\r\n      </div>\r\n      <div class=\"box g-box blue\">\r\n        <p style=\"text-align: center;\">这是一个大盒子</p>\r\n      </div>\r\n    </div>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport * as echarts from 'echarts'\r\nrequire('echarts/theme/macarons')\r\n\r\nexport default {\r\n  name: 'Home',\r\n  data() {\r\n    return {\r\n      //livestock牲畜坐标\r\n      // livestockCoordinates:[],\r\n      //圆心坐标\r\n      centerl:[],\r\n      //用户经纬度\r\n      lon: null,\r\n      lat: null,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.createPieChart();\r\n    this.createBarChart();\r\n    this.initMap();\r\n\r\n      // 获取在线和离线圆环的元素\r\n      const onlineCircle = document.getElementById('online');\r\n    const offlineCircle = document.getElementById('offline');\r\n\r\n    // 设置在线圆环的 stroke-dasharray 属性为 \"70, 30\"，表示70%在线\r\n    onlineCircle.setAttribute('stroke-dasharray', '0, 565');\r\n    // 设置离线圆环的 stroke-dasharray 属性为 \"30, 70\"，表示30%离线\r\n    offlineCircle.setAttribute('stroke-dasharray', '0, 240');\r\n\r\n  },\r\n//   created() {\r\n//     this.initMap();\r\n// },\r\n\r\n  methods: {\r\n\r\n    initMap() {\r\n      this.map = new AMap.Map(\"mapContainer\", {\r\n        zoom: 8,\r\n        center: [123.414875, 41.908154],\r\n        resizeEnable: true\r\n      });\r\n      console.log(\"调用了吗\")\r\n\r\n          \r\n  },\r\n\r\n  createPieChart() {\r\n  var ctx = document.getElementById('pieChart');\r\n  var myChart = echarts.init(ctx, 'macarons');\r\n  var option = {\r\n    grid: {\r\n      left: '20%', // 设置左边距\r\n      containLabel: true // 是否包含坐标轴的刻度标签\r\n    },\r\n    series: [{\r\n      type: 'pie',\r\n      data: [\r\n        { name: '牛', value: 20, itemStyle: { color: '#FF5722' } }, // 使用不同的颜色区分\r\n        { name: '猪', value: 30, itemStyle: { color: '#2196F3' } },\r\n        { name: '羊', value: 25, itemStyle: { color: '#4CAF50' } },\r\n        { name: '鸡', value: 15, itemStyle: { color: '#FFC107' } }\r\n      ]\r\n    }]\r\n  };\r\n  myChart.setOption(option);\r\n},\r\n\r\n\r\ncreateBarChart() {\r\n  var ctx = document.getElementById('barChart');\r\n  var myChart = echarts.init(ctx); // 不指定主题，使用默认主题\r\n  var option = {\r\n    backgroundColor: '#fff', // 设置背景色为白色\r\n    xAxis: {\r\n      type: 'category',\r\n      data: ['牛', '猪', '羊', '鸡'],\r\n      axisLine: {\r\n        lineStyle: {\r\n          color: '#999', // 设置横坐标轴线颜色\r\n          width: 2 // 设置横坐标轴线宽度\r\n        }\r\n      }\r\n    },\r\n    yAxis: {\r\n      type: 'value',\r\n      axisLine: {\r\n        lineStyle: {\r\n          color: '#999', // 设置纵坐标轴线颜色\r\n          width: 2 // 设置纵坐标轴线宽度\r\n        }\r\n      }\r\n    },\r\n    series: [{\r\n      data: [20, 15, 25, 18],\r\n      type: 'bar',\r\n      barWidth: 20, // 设置更细的柱状图宽度\r\n      itemStyle: {\r\n        color: new echarts.graphic.LinearGradient(0, 0, 0, 1, [{\r\n            offset: 0,\r\n            color: '#4b9eff' // 渐变色起始颜色\r\n          },\r\n          {\r\n            offset: 1,\r\n            color: '#b4ffff' // 渐变色结束颜色\r\n          }\r\n        ]),\r\n        borderWidth: 1, // 添加边框宽度\r\n        borderColor: '#fff', // 设置边框颜色\r\n        shadowBlur: 5, // 添加阴影效果\r\n        shadowColor: 'rgba(0, 0, 0, 0.3)' // 设置阴影颜色\r\n      },\r\n      label: {\r\n        show: true,\r\n        position: 'top', // 标签位置在柱状图顶部\r\n        color: '#fff', // 标签文字颜色\r\n        fontSize: 14, // 标签文字大小\r\n        fontWeight: 'bold' // 标签文字加粗\r\n      },\r\n      emphasis: {\r\n        itemStyle: {\r\n          shadowBlur: 10, // 添加阴影效果\r\n          shadowColor: 'rgba(0, 0, 0, 0.5)' // 设置阴影颜色\r\n        }\r\n      }\r\n    }],\r\n    animationDuration: 3000, // 设置更长的动画持续时间\r\n    animationEasing: 'elasticOut' // 设置弹性缓动函数\r\n  };\r\n  myChart.setOption(option);\r\n}\r\n\r\n\r\n// createBarChart() {\r\n//   var ctx = document.getElementById('barChart'); \r\n//   var myChart = echarts.init(ctx, 'dark'); // 使用 dark 主题\r\n//   var option = {\r\n//     xAxis: {\r\n//       type: 'category',\r\n//       data: ['牛', '猪', '羊', '鸡'],\r\n//       axisLine: {\r\n//         lineStyle: {\r\n//           color: '#999', // 设置横坐标轴线颜色\r\n//           width: 2 // 设置横坐标轴线宽度\r\n//         }\r\n//       }\r\n//     },\r\n//     yAxis: {\r\n//       type: 'value',\r\n//       axisLine: {\r\n//         lineStyle: {\r\n//           color: '#999', // 设置纵坐标轴线颜色\r\n//           width: 2 // 设置纵坐标轴线宽度\r\n//         }\r\n//       }\r\n//     },\r\n//     series: [{\r\n//       data: [20, 15, 25, 18],\r\n//       type: 'bar',\r\n//       barWidth: 30, // 设置柱状图宽度\r\n//       itemStyle: {\r\n//         color: new echarts.graphic.LinearGradient(0, 0, 0, 1, [{\r\n//             offset: 0,\r\n//             color: '#ff9f7f' // 渐变色起始颜色\r\n//           },\r\n//           {\r\n//             offset: 1,\r\n//             color: '#ff3c54' // 渐变色结束颜色\r\n//           }\r\n//         ])\r\n//       },\r\n//       emphasis: {\r\n//         itemStyle: {\r\n//           shadowBlur: 10, // 添加阴影效果\r\n//           shadowColor: 'rgba(0, 0, 0, 0.5)' // 设置阴影颜色\r\n//         }\r\n//       }\r\n//     }],\r\n//     animationDuration: 2000, // 设置动画持续时间\r\n//     animationEasing: 'cubicInOut' // 设置动画缓动函数\r\n//   };\r\n//   myChart.setOption(option);\r\n// }\r\n\r\n    // createBarChart() {\r\n    //   var ctx = document.getElementById('barChart'); \r\n    //   var myChart = echarts.init(ctx, 'macarons');\r\n    //   var option = {\r\n    //     xAxis: {\r\n    //       type: 'category',\r\n    //   data: ['牛', '猪', '羊', '鸡'] // 添加猪、羊、鸡到横坐标\r\n    // },\r\n    // yAxis: {\r\n    //   type: 'value'\r\n    // },\r\n    // series: [{\r\n    //   data: [20, 15, 25, 18], // 对应猪、羊、鸡的数量数据\r\n    //   type: 'bar'\r\n    //     }]\r\n    //   };\r\n    //   myChart.setOption(option);\r\n    // }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* 样式可以根据您的需求自行调整 */\r\n\r\n.app-container {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  /* max-width: 1330px; */\r\n  margin: 0 auto;/* 宽度自适应 */\r\n  height: auto; /* 高度自适应 */  \r\n  background-color: #b5c9dc; /* 深蓝色 */\r\n  /*  \r\n    background-image: url('../assets/images/background.png'); // 绝对路径\r\n    background-size: cover; // 背景图片尺寸覆盖整个容器 \r\n  background-repeat: no-repeat; // 防止背景图片重复   \r\n  */\r\n}\r\n\r\n\r\n.box {\r\n  margin-bottom: 20px;\r\n  padding: 20px;\r\n  box-sizing: border-box;\r\n  border-radius: 20px; /* 设置圆角大小 */\r\n  box-shadow: 0 10px 20px rgba(0, 0, 0, 0.2); /* 添加立体阴影效果 */\r\n}\r\n\r\n.blue {\r\n  background-color: #ffffff; /* 蓝色 */\r\n  color: #000000; /* 文字颜色 */\r\n}\r\n\r\n.map-container {\r\n  width: 100%;\r\n  height: 90%;\r\n  /* width: 455px;\r\n  height: 390px; */\r\n  margin-top: 38px; /* 向下移动 20px */\r\n}\r\n\r\n.left-column,\r\n.middle-column,\r\n.right-column {\r\n  flex-grow: 1; /* 等分剩余空间 */\r\n  display: flex;\r\n  flex-direction: column;\r\n  width: calc(33.33% - 10px); /* 计算宽度，考虑到间隙 */\r\n  margin-right: 10px; /* 为了留出间隙 */\r\n\r\n}\r\n\r\n.b-box,\r\n.i-box,\r\n.g-box,\r\n.small-box {\r\n  flex: 1;\r\n}\r\n\r\n\r\n\r\n.bigger-box {\r\n  height: 470px; /* 调整更大的盒子的高度 */\r\n\r\n  font-size: 24px; /* 调整更大的盒子的文字大小 */\r\n  line-height: 10px; /* 垂直居中文本，这里是盒子高度的一半 */\r\n  vertical-align: middle; /* 垂直居中文本 */\r\n  text-align: center; /* 水平居中文本 */\r\n  /* filter: blur(1px); 调整此处的像素值以达到所需的模糊程度 */\r\n  border-radius: 20px; /* 设置圆角大小 */\r\n  box-shadow: 0 10px 20px rgba(0, 0, 0, 0.2); /* 添加立体阴影效果 */\r\n}\r\n\r\n\r\n\r\n\r\n\r\n.monitoring-table {\r\n  border-collapse: collapse; /* 合并边框 */\r\n  width: 100%;\r\n}\r\n\r\n.monitoring-table th,\r\n.monitoring-table td {\r\n  border: 1px solid #ddd; /* 设置单元格边框样式 */\r\n  padding: 8px; /* 设置单元格内边距 */\r\n  text-align: left; /* 文字左对齐 */\r\n}\r\n\r\n.monitoring-table th {\r\n  background-color: #f2f2f2; /* 设置表头背景颜色 */\r\n  color: #333; /* 设置表头文字颜色 */\r\n}\r\n\r\n.status-circle-container {\r\n        position: relative;\r\n        width: 200px;\r\n        height: 200px;\r\n        display: flex;\r\n        justify-content: center;\r\n        align-items: center;\r\n        background-color: #282c34;\r\n        border-radius: 50%;\r\n        box-shadow: 0 0 20px rgba(0, 0, 0, 0.3);\r\n        overflow: hidden;\r\n    }\r\n\r\n    .status-circle {\r\n        fill: none;\r\n        stroke-width: 20;\r\n        stroke-linecap: round;\r\n        transition: stroke-dasharray 0.8s ease;\r\n    }\r\n\r\n    .status-text {\r\n        font-size: 18px;\r\n        fill: #fff;\r\n        font-family: 'Arial', sans-serif;\r\n    }\r\n\r\n\r\n</style>\r\n\r\n"]}]}